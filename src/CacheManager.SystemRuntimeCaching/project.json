{
  "version": "0.9.4-*",
  "description": "CacheManager extension package which adds System.Runtime.Caching.MemoryCache as an option for a local in-memory cache layer. CacheManager is an open source caching abstraction layer for .NET written in C#. It supports various cache providers and implements many advanced features.",
  "packOptions": {
    "licenseUrl": "https://github.com/MichaCo/CacheManager/blob/master/LICENSE",
    "projectUrl": "http://cachemanager.michaco.net",
    "repository": {
      "type": "git",
      "url": "https://github.com/MichaCo/CacheManager"
    },
    "summary": "CacheManager extension package which adds System.Runtime.Caching.MemoryCache as an option for a local in-memory cache layer.",
    "tags": [ "Caching", "Cache", "CacheManager", "Distributed Cache", "StackExchange Redis", "Azure AppFabric", "Memcached" ]
  },
  "authors": [ "MichaConrad" ],
  "buildOptions": {
    "allowUnsafe": false,
    "languageVersion": "csharp6",
    "warningsAsErrors": true,
    "xmlDoc": true,
    "keyFile": "../../tools/key.snk"
  },
  "copyright": "Copyright (c) 2016 MichaConrad",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE" ],
        "optimize": true
      }
    }
  },
  "dependencies": {
    "CacheManager.Core": { "target": "project" }
  },
  "frameworks": {
    "net40": {
      "frameworkAssemblies": {
        "System.Runtime.Caching": "4.0.0.0"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Runtime.Caching": "4.0.0.0"
      }
    }
  }
}
