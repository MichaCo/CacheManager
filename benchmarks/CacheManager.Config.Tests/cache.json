{
  "$schema": "http://cachemanager.michaco.net/schemas/cachemanager.json#",
  "redis": [
    {
      "key": "redisConnection",      
      "endpoints": [
        {
          "host": "localhost",
          "port": 6379
        }
      ],
      "allowAdmin": true,
      "database": 11
    }
  ],
  "cacheManagers": [
    {
      "maxRetries": 1000,
      "name": "cachename",
      "retryTimeout": 100,
      "updateMode": "Up",
      "backplane": {
        "key": "redisConnection",
        "knownType": "Redis",
        "channelName": "test"
      },
      "loggerFactory": {
        "knownType": "Microsoft"
      },
      "serializer": {
        "knownType": "Json"
      },
      "handles": [
        //{
        //  "knownType": "Dictionary",
        //  "enablePerformanceCounters": true,
        //  "enableStatistics": true,
        //  "expirationMode": "Absolute",
        //  "expirationTimeout": "0:0:23",
        //  "isBackplaneSource": false,
        //  "name": "sys cache"
        //},
        {
          "knownType": "Redis",
          "key": "redisConnection",
          "isBackplaneSource": true,
          "expirationMode": "Sliding",
          "expirationTimeout": "00:10:00"
        }
      ]
    }
  ]
}
